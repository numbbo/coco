[package]
name = "coco-sys"
version = "0.6.0"
license = "MIT OR Apache-2.0"
authors = ["Leopold Luley", "Olaf Mersmann"]
description = "Generated Rust bindings for COCO benchmarking framework"
repository = "https://github.com/numbbo/coco"
keywords = ["coco", "benchmark", "blackbox", "bindings"]
categories = ["science"]
edition = "2021"
links = "coco"
include = ["build.rs", "src/", "vendor/", "wrapper.c", "wrapper.h"]

[build-dependencies]
bindgen = "0.64.0"
cc = "1.0"
